{
  "name": "cloudflare-bundler",
  "version": "0.2.1",
  "description": "Bundle a Cloudflare worker for deployment",
  "author": "Alec Larson",
  "license": "MIT",
  "repository": "alloc/cloudflare-bundler",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^2.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@types/html-minifier": "^4.0.0",
    "axios": "^0.21.0",
    "esbuild": "^0.8.0",
    "etag": "^1.8.1",
    "html-minifier": "^4.0.0",
    "recrawl-sync": "^2.0.0",
    "rollup": "^2.35.1",
    "rollup-plugin-esbuild": "npm:@cush/rollup-plugin-esbuild@2.5.2-canary.138",
    "rollup-plugin-terser": "^7.0.2",
    "strip-bom": "^4.0.0",
    "toml": "^3.0.0"
  },
  "devDependencies": {
    "@auto-it/conventional-commits": "^10.5.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/etag": "^1.8.0",
    "prettier": "^2.0.5",
    "rollup-plugin-dts": "^1.4.10",
    "typescript": "^4.0.0"
  },
  "keywords": [
    "bundle",
    "bundler",
    "cloudflare",
    "worker"
  ],
  "auto": {
    "plugins": [
      "npm",
      "conventional-commits"
    ]
  }
}
